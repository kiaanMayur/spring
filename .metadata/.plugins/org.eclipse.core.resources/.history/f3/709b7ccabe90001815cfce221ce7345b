
public class Person { 
	
	private String name;
	private Date dob;
	public Person(String name, int day, int month, int year) {
		
		this.name = name;
		dob.setDay(day);
		dob.setMonth(day);
		dob.setYear(day);
	}
	
	public void display() {
		
		System.out.println("Name: " + name + "\nDate of Birth");
	}
	
	
	

}
